<%= form_with(@product,local: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name,class: 'form-control' %>

  <%= f.label :price %>
  <%= f.text_field :price,class: 'form-control' %>

  <%= f.label :picture %>
  <%= f.file_field :picture, id: :roduct_picture %>

  <%= f.submit '商品登録',class: 'btn btn-primary' %>
<% end %>
